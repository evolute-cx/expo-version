{
  "name": "expo-version",
  "version": "1.0.1",
  "description": "Version management tool for Expo React Native apps, similar to npm version but for expo apps",
  "main": "dist/index.js",
  "bin": {
    "expo-version": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "dev": "tsc --watch",
    "test": "jest",
    "prepack": "npm run build"
  },
  "keywords": [
    "expo",
    "react-native",
    "version",
    "versioning",
    "semver",
    "git",
    "eas",
    "cli",
    "build-tools"
  ],
  "author": "Daniel Engelhardt - Evolute CX GmbH",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "dependencies": {
    "semver": "^7.5.4",
    "prompts": "^2.4.2",
    "chalk": "^4.1.2",
    "simple-git": "^3.20.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/prompts": "^2.4.4",
    "@types/semver": "^7.5.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evolute-cx/expo-version.git"
  },
  "bugs": {
    "url": "https://github.com/evolute-cx/expo-version/issues"
  },
  "homepage": "https://github.com/evolute-cx/expo-version#readme"
}
